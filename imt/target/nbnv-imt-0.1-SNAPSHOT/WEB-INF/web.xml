<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>NBN Gateway - Interactive Map Tool</display-name>
    <servlet>
        <servlet-name>DatasetSearch</servlet-name>
        <servlet-class>nbn.webmapping.json.search.dataset.AutoCompleteDatasetSearchJSON</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TaxonSearch</servlet-name>
        <servlet-class>nbn.webmapping.json.search.taxon.AutoCompleteTaxonSearchJSON</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TreeWidgetGenerator</servlet-name>
        <servlet-class>nbn.webmapping.json.treewidget.servlet.DHTMLXTreeWidgetGeneratorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrganisationAcknowledgementServlet</servlet-name>
        <servlet-class>nbn.webmapping.json.metadata.acknowledgement.OrganisationAcknowledgementServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>nbn.webmapping.json.login.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EntityResolver</servlet-name>
        <servlet-class>nbn.webmapping.json.entity.EntityResolver</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NonViewableTaxonDatasetsServlet</servlet-name>
        <servlet-class>nbn.webmapping.json.metadata.unviewable.NonViewableTaxonDatasetsServlet</servlet-class>
    </servlet>
    <!--START PICKER SERVLET DEFINITIONS-->
    <servlet>
        <servlet-name>DatasetPickerServlet</servlet-name>
        <servlet-class>nbn.webmapping.json.picker.impl.DatasetPickerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SingleSpeciesPickerServlet</servlet-name>
        <servlet-class>nbn.webmapping.json.picker.impl.SingleSpeciesPickerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DesignationPickerServlet</servlet-name>
        <servlet-class>nbn.webmapping.json.picker.impl.DesignationPickerServlet</servlet-class>
    </servlet>
    <!--END PICKER SERVLET DEFINITIONS-->
    <!--START PICKER SERVLET MAPPINGS-->
    <servlet-mapping>
        <servlet-name>DatasetPickerServlet</servlet-name>
        <url-pattern>/DatasetPickerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SingleSpeciesPickerServlet</servlet-name>
        <url-pattern>/SingleSpeciesPickerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DesignationPickerServlet</servlet-name>
        <url-pattern>/DesignationPickerServlet</url-pattern>
    </servlet-mapping>
    <!--END PICKER SERVLET MAPPINGS-->
    <servlet-mapping>
        <servlet-name>DatasetSearch</servlet-name>
        <url-pattern>/DatasetSearch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TaxonSearch</servlet-name>
        <url-pattern>/TaxonSearch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TreeWidgetGenerator</servlet-name>
        <url-pattern>/TreeWidgetGenerator</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrganisationAcknowledgementServlet</servlet-name>
        <url-pattern>/OrganisationAcknowledgementServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EntityResolver</servlet-name>
        <url-pattern>/EntityResolver</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NonViewableTaxonDatasetsServlet</servlet-name>
        <url-pattern>/NonViewableTaxonDatasetsServlet</url-pattern>
    </servlet-mapping>
</web-app>
