<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>NBN Gateway GIS</display-name>
    <servlet>
        <servlet-name>MapServerServlet</servlet-name>
        <servlet-class>uk.gov.nbn.data.gis.MapServerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MapServerServlet</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
    <filter>
        <filter-name>MapAuthenticationFilter</filter-name>
        <filter-class>uk.gov.nbn.data.gis.MapServerNBNAuthenticationFilter</filter-class>
    </filter>
</web-app>
