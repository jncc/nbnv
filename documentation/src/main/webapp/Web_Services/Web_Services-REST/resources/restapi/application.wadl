          <wadl:representation mediaType="application/xml" element="user"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A Response object detailing the success or failure of the action]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
