    <wadl:resource path="/api/user/login">
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[Log the indicated user into the system]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="username" style="query">
            <wadl:doc>
              <![CDATA[The username of the user]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="password" style="query">
            <wadl:doc>
              <![CDATA[The password of the user]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="remember" style="query">
            <wadl:doc>
              <![CDATA[Whether the browser should remember this data (stay
logged in)]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A Response object detailing the success or failure of the action]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Log the indicated user into the system, via a POST]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="remember" style="query">
            <wadl:doc>
              <![CDATA[Whether the browser should remember this data (stay
logged in)]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A Response object detailing the success or failure of the action]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
