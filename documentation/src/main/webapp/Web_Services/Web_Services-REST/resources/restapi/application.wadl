    <wadl:resource path="/api/taxonObservations">
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[Returns a list of Taxon Observations matching the given serach parameters]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="startYear" style="query">
            <wadl:doc>
              <![CDATA[The start year of the desired range]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="endYear" style="query">
            <wadl:doc>
              <![CDATA[The end year of the desired range]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="datasetKey" style="query">
            <wadl:doc>
              <![CDATA[Datasets to search in]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="ptvk" style="query">
            <wadl:doc>
              <![CDATA[Taxon Version Keys to search for]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="spatialRelationship" style="query">
            <wadl:doc>
              <![CDATA[Any spatial relationship information required]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="featureID" style="query">
            <wadl:doc>
              <![CDATA[Any required feature ID]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="sensitive" style="query">
            <wadl:doc>
              <![CDATA[If the results should include sensitive records or not]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="designation" style="query">
            <wadl:doc>
              <![CDATA[Any required designations]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="taxonOutputGroup" style="query">
            <wadl:doc>
              <![CDATA[Any required taxon output groups]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="orgSuppliedList" style="query"/>
          <wadl:param name="gridRef" style="query">
            <wadl:doc>
              <![CDATA[Any grid references to search within]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="polygon" style="query">
            <wadl:doc>
              <![CDATA[WKT WGS-84 polygon filter]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="absence" style="query">
            <wadl:doc>
              <![CDATA[Whether the results should be limited to just absence records (true)
or just presence records (false).  If this parameter is missing then both absence
and presence records are returned.  Valid values are 'true' and 'false']]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A list of Taxon Observations conforming to the provided search
parameters]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Returns a list of Taxon Observations matching the given serach parameters]]>
        </wadl:doc>
        <wadl:request/>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A list of Taxon Observations conforming to the provided search
parameters]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
