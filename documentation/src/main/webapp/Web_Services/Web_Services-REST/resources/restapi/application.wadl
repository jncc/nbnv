    <wadl:resource path="/api/taxa">
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[Search for a Taxon Record given a search term, this is a SOLR search
function]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="rows" style="query">
            <wadl:doc>
              <![CDATA[The number of rows to display per page]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="start" style="query">
            <wadl:doc>
              <![CDATA[The starting page]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="taxonOutputGroupKey" style="query">
            <wadl:doc>
              <![CDATA[A List of Taxon Output Groups]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="sort" style="query">
            <wadl:doc>
              <![CDATA[Whether we should sort the results]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="order" style="query">
            <wadl:doc>
              <![CDATA[The order in which we should sort the results]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="prefered" style="query">
            <wadl:doc>
              <![CDATA[If we should restrict the results to preferred taxon
records]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="q" style="query">
            <wadl:doc>
              <![CDATA[A given search term]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[Search results for a given search term]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
