    <wadl:resource path="/api/search">
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[Unrestricted search, searches for any reference to a given search term in
all searchable objects in the database]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="rows" style="query">
            <wadl:doc>
              <![CDATA[The number of rows to show in each page of search results]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="start" style="query">
            <wadl:doc>
              <![CDATA[The page that the user wants to start displaying the results
at]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="sort" style="query">
            <wadl:doc>
              <![CDATA[If we should sort the results or not]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="order" style="query">
            <wadl:doc>
              <![CDATA[The order in which we should sort the results]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="q" style="query">
            <wadl:doc>
              <![CDATA[The search term]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="exclude" style="query">
            <wadl:doc>
              <![CDATA[a record type to exclude from the search results (eg the
IMT requires the exclusion of records that are of type 'organisation')]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[Search Results for this search term]]>
          </wadl:doc>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
