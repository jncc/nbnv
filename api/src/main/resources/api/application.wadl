<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:application xmlns:ns2="http://wadl.dev.java.net/2009/02">
    <ns2:doc xmlns:jersey="http://jersey.java.net/" jersey:generatedBy="Jersey: 1.15 10/30/2012 02:40 PM"/>
    <ns2:grammars/>
    <ns2:resources base="http://localhost:8084/api/">
        <ns2:resource path="/features">
            <ns2:doc>A jersey resource which provides bounding boxes for certain features on the 
 gateway</ns2:doc>
            <ns2:resource path="/{id}">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:string">
                    <ns2:doc>A String Based Feature ID</ns2:doc>
                </ns2:param>
                <ns2:method id="getFeature" name="GET">
                    <ns2:doc>Return a specified feature with a string feature id</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A specified feature with a string feature id</ns2:doc>
                        <ns2:representation element="Feature" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/siteBoundaries">
                <ns2:method id="getSiteBoundaries" name="GET">
                    <ns2:doc>Return a list of all Site Boundaries from the data warehouse (Should this 
 be here?)</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A list of all Site Boundaries from the data warehouse</ns2:doc>
                        <ns2:representation element="List&lt;SiteBoundary&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{id: [0-9]+}">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:int">
                    <ns2:doc>A Numerical Feature ID</ns2:doc>
                </ns2:param>
                <ns2:method id="getFeatureId" name="GET">
                    <ns2:doc>Return a specified feature with a numerical ID</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A specified feature with a numerical ID</ns2:doc>
                        <ns2:representation element="Feature" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
        </ns2:resource>
        <ns2:resource path="/taxonDatasets">
            <ns2:method id="getDatasetList" name="GET">
                <ns2:doc>Return a list of all Taxon Datasets from the data warehouse</ns2:doc>
                <ns2:response status="200">
                    <ns2:doc>A list of all Taxon Datasets from the data warehouse</ns2:doc>
                    <ns2:representation element="List&lt;TaxonDataset&gt;" mediaType="application/json"/>
                </ns2:response>
            </ns2:method>
            <ns2:resource path="/{id}/recordsPerYear">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:string">
                    <ns2:doc>A dataset key</ns2:doc>
                </ns2:param>
                <ns2:method id="getRecordsPerYearByDatasetKey" name="GET">
                    <ns2:doc>Return a list of yearly stats for specific dataset</ns2:doc>
                    <ns2:response status="200">
