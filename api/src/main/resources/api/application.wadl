                        <ns2:doc>A list of all Organisation Access Requests for a given 
 organisation</ns2:doc>
                        <ns2:representation element="List&lt;OrganisationAccessRequest&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/requests/history/{dataset}">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="dataset" style="template" type="xs:string">
                    <ns2:doc>The dataset being queried</ns2:doc>
