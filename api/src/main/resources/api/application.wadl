                <ns2:response status="200">
                    <ns2:doc>List of Datasets</ns2:doc>
                    <ns2:representation element="List&lt;Dataset&gt;" mediaType="application/json"/>
                </ns2:response>
            </ns2:method>
            <ns2:resource path="/{id}">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:string">
                    <ns2:doc>Key of a dataset i.e. GA000466</ns2:doc>
                </ns2:param>
                <ns2:method id="getDatasetByID" name="GET">
                    <ns2:doc>Returns a specific dataset from the data warehouse</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A specific dataset from the data warehouse</ns2:doc>
                        <ns2:representation element="Dataset" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
                <ns2:method id="updateDataset" name="POST">
                    <ns2:doc>If the current user is an admin of the specified dataset update the 
 dataset metadata with the values contained in the POSTed form otherwise
 returns a 403 Forbidden error</ns2:doc>
                    <ns2:request>
                        <ns2:representation mediaType="application/x-www-form-urlencoded">
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="title" style="query" type="xs:string">
<ns2:doc>The dataset title</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="description" style="query" type="xs:string">
<ns2:doc>The dataset description</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="captureMethod" style="query" type="xs:string">
<ns2:doc>The dataset Capture Method</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="purpose" style="query" type="xs:string">
<ns2:doc>The datasets Purpose</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="geographicalCoverage" style="query" type="xs:string">
<ns2:doc>The Geographical Coverage of the dataset</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="quality" style="query" type="xs:string">
<ns2:doc>The quality of recording in this dataset</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="additionalInformation" style="query" type="xs:string">
<ns2:doc>Any Additional Information about this dataset</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="accessConstraints" style="query" type="xs:string">
<ns2:doc>Any Access Constraints applicable to this dataset</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="useConstraints" style="query" type="xs:string">
<ns2:doc>Any Use Constraints applicable to this dataset</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="temporalCoverage" style="query" type="xs:string">
<ns2:doc>The Temporal Coverage of this dataset</ns2:doc>
                            </ns2:param>
                        </ns2:representation>
                    </ns2:request>
                    <ns2:response status="200">
                        <ns2:doc>An OpResult detailing the success or failure of the action</ns2:doc>
                        <ns2:representation element="OpResult" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{id}/edit">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:string">
                    <ns2:doc>ID of a dataset i.e. GA000466</ns2:doc>
                </ns2:param>
                <ns2:method id="getEditDatasetByID" name="GET">
                    <ns2:doc>Returns a selected dataset from the core database if the current user is
 an admin of the specified dataset otherwise returns a 403 Forbidden error</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A selected dataset from the core database if the current user is
 an admin of the specified dataset</ns2:doc>
                        <ns2:representation element="Dataset" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/latest">
                <ns2:method id="getLastUpdatedDatasets" name="GET">
                    <ns2:doc>Returns a list of the last 10 most recently uploaded datasets</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>a list of the last 10 most recently uploaded datasets</ns2:doc>
                        <ns2:representation element="List&lt;Dataset&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/latest.rss">
                <ns2:method id="getLastUpdatedDatasetsFeed" name="GET">
                    <ns2:doc>Returns an atom+xml feed of the latest 10 most recently uploaded datasets</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>an atom+xml feed of the latest 10 most recently uploaded datasets</ns2:doc>
                        <ns2:representation element="Feed" mediaType="application/atom+xml"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{datasetKey}/accessPositions">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="datasetKey" style="template" type="xs:string">
                    <ns2:doc>Key of a dataset i.e. GA000466</ns2:doc>
                </ns2:param>
                <ns2:method id="getAttributesByDatasetKey" name="GET">
                    <ns2:doc>Returns a list of attributes relating to the specified dataset</ns2:doc>
                    <ns2:response status="200">
                        <ns2:representation element="List&lt;String&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{datasetKey}/resolutionData">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="datasetKey" style="template" type="xs:string">
                    <ns2:doc>Key of a dataset i.e. GA000466</ns2:doc>
                </ns2:param>
                <ns2:method id="getDatasetResolution" name="GET">
                    <ns2:doc>Returns a list of DatasetResolutionRecordCount giving the resolution data
 for the specified dataset</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A list of DatasetResolutionRecordCount giving the resolution data
 for the specified dataset</ns2:doc>
                        <ns2:representation element="List&lt;DatasetResolutionRecordCount&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{datasetKey}/isAdmin">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="datasetKey" style="template" type="xs:string">
                    <ns2:doc>Key of a dataset i.e. GA000466</ns2:doc>
                </ns2:param>
                <ns2:method id="isDatasetAdmin" name="GET">
                    <ns2:doc>Return boolean stating if the user is an administrator of this dataset
 used in the freemarker templates to optionally include links to edit
 dataset / survey metadata</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>True if the user is admin of this dataset</ns2:doc>
                        <ns2:representation element="boolean" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/adminable">
                <ns2:method id="adminableDatasets" name="GET">
                    <ns2:doc>Returns a list of datasets the user can admin</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>List of datasets</ns2:doc>
                        <ns2:representation element="List&lt;Dataset&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{datasetKey}/surveys/{survey}">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="datasetKey" style="template" type="xs:string">
                    <ns2:doc>Key of the parent dataset i.e. GA000466</ns2:doc>
                </ns2:param>
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="survey" style="template" type="xs:int">
                    <ns2:doc>Key of the survey to have its metadata edited</ns2:doc>
                </ns2:param>
                <ns2:method id="getJson" name="GET">
                    <ns2:doc>Get survey metadata from the core database if the user is an admin of the
 specified parent dataset otherwise return a 403 Forbidden error</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>The specified surveys metadata from the core database</ns2:doc>
                        <ns2:representation element="Survey" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
                <ns2:method id="putJson" name="POST">
                    <ns2:doc>If the current user is an admin of the specified dataset update the 
 specified surveys metadata with the values contained in the POSTed form</ns2:doc>
                    <ns2:request>
                        <ns2:representation mediaType="application/x-www-form-urlencoded">
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="providerKey" style="query" type="xs:string">
<ns2:doc>Key provided by the owner of the survey (external id)</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="title" style="query" type="xs:string">
<ns2:doc>The survey title</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="description" style="query" type="xs:string">
<ns2:doc>The survey description</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="geographicalCoverage" style="query" type="xs:string">
<ns2:doc>The Geographical Coverage of the survey</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="temporalCoverage" style="query" type="xs:string">
<ns2:doc>The Temporal Coverage of this survey</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="dataQuality" style="query" type="xs:string">
<ns2:doc>The quality of recording in this survey</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="dataCaptureMethod" style="query" type="xs:string">
<ns2:doc>The survey Capture Method</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="purpose" style="query" type="xs:string">
<ns2:doc>The surveys purpose</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="additionalInformation" style="query" type="xs:string">
<ns2:doc>Any Additional Information about this survey</ns2:doc>
                            </ns2:param>
                        </ns2:representation>
                    </ns2:request>
                    <ns2:response status="200">
                        <ns2:doc>A Response object detailing the success or failure of the action</ns2:doc>
                        <ns2:representation element="Response" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
        </ns2:resource>
        <ns2:resource path="/siteBoundaryCategories">
            <ns2:method id="get" name="GET">
                <ns2:doc>Return a list of all site boundary categories from the data warehouse</ns2:doc>
                <ns2:response status="200">
                    <ns2:doc>A list of all site boundary categories from the data warehouse</ns2:doc>
                    <ns2:representation element="List&lt;SiteBoundaryCategory&gt;" mediaType="application/json"/>
                </ns2:response>
            </ns2:method>
            <ns2:resource path="/{id}">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:int">
                    <ns2:doc>A Site Boundary Category ID</ns2:doc>
                </ns2:param>
                <ns2:method id="getByID" name="GET">
                    <ns2:doc>Return a specific site boundary category</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A specific site boundary category</ns2:doc>
                        <ns2:representation element="SiteBoundaryCategory" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
        </ns2:resource>
        <ns2:resource path="/user/userAccesses">
            <ns2:resource path="/requests">
                <ns2:method id="createRequest" name="PUT">
                    <ns2:doc>Create a Access Request for a user</ns2:doc>
                    <ns2:request>
                        <ns2:representation mediaType="application/json"/>
                    </ns2:request>
                    <ns2:response status="200">
                        <ns2:doc>A Response object detailing the success or failure of the action</ns2:doc>
                        <ns2:representation element="Response" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
                <ns2:method id="getRequests" name="GET">
                    <ns2:doc>Return a list of access requests made by a user</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A List of User Access Requests made by a user</ns2:doc>
                        <ns2:representation element="List&lt;UserAccessRequest&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/requests/granted">
                <ns2:method id="getGrantedRequests" name="GET">
                    <ns2:doc>Return a list of access requests made by a user that have been granted</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A List of User Access Requests made by a user that have been 
 granted</ns2:doc>
                        <ns2:representation element="List&lt;UserAccessRequest&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/requests/admin">
                <ns2:method id="getRequestsForAdmin" name="GET">
                    <ns2:doc>Return a list of access requests made that a user has admin rights over</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A List of User Access Requests that a user has admin rights over</ns2:doc>
                        <ns2:representation element="List&lt;UserAccessRequest&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/requests/admin/pending">
                <ns2:method id="getRequestsPendingForAdmin" name="GET">
                    <ns2:doc>Return a list of access requests made that a user has admin rights over 
 and are pending</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A List of User Access Requests that a user has admin rights over 
 and are still pending</ns2:doc>
                        <ns2:representation element="List&lt;UserAccessRequest&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/requests/admin/granted">
                <ns2:method id="getRequestsGrantedForAdmin" name="GET">
                    <ns2:doc>Return a list of access requests made that a user has admin rights over
 and have been granted</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A List of User Access Requests that a user has admin rights over 
 and have been granted</ns2:doc>
                        <ns2:representation element="List&lt;UserAccessRequest&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
                <ns2:method id="createGrant" name="PUT">
                    <ns2:request>
                        <ns2:representation mediaType="application/json"/>
                    </ns2:request>
                    <ns2:response>
                        <ns2:representation mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/requests/admin/denied">
                <ns2:method id="getRequestsDeniedForAdmin" name="GET">
                    <ns2:doc>Return a list of access requests made that a user has admin rights over
 and have been denied</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A List of User Access Requests that a user has admin rights over 
 and have been denied</ns2:doc>
                        <ns2:representation element="List&lt;UserAccessRequest&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/requests/{id}">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:int">
                    <ns2:doc>A filter ID for this request</ns2:doc>
                </ns2:param>
                <ns2:method id="updateRequest" name="POST">
                    <ns2:doc>Update a specified User Access Request if the user has admin rights over
 the request, otherwise return a 403 Forbidden error</ns2:doc>
                    <ns2:request>
                        <ns2:representation mediaType="application/x-www-form-urlencoded">
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="action" style="query" type="xs:string">
<ns2:doc>What action to take in this update</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="reason" style="query" type="xs:string">
<ns2:doc>The reason that this action has been taken</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="expires" style="query" type="xs:string" default="">
<ns2:doc>If the action should expire after a certain date</ns2:doc>
                            </ns2:param>
                        </ns2:representation>
                    </ns2:request>
                    <ns2:response status="200">
                        <ns2:doc>A Response object detailing the success or failure of the action</ns2:doc>
                        <ns2:representation element="Response" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
                <ns2:method id="getRequest" name="GET">
                    <ns2:doc>Return a specific User Access Request as long as the current user has 
 admin rights over that request, otherwise return a 403 Forbidden error</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A User Access Request that a user has admin rights over</ns2:doc>
                        <ns2:representation element="UserAccessRequest" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
                <ns2:method id="editRequest" name="PUT">
                    <ns2:doc>Edit an existing User Access Request</ns2:doc>
                    <ns2:request>
                        <ns2:representation mediaType="application/json"/>
                    </ns2:request>
                    <ns2:response status="200">
                        <ns2:doc>A Response object detailing the success or failure of the action</ns2:doc>
                        <ns2:representation element="Response" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/requests/history/{dataset}">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="dataset" style="template" type="xs:string">
                    <ns2:doc>The dataset being queried</ns2:doc>
                </ns2:param>
                <ns2:method id="getHistory" name="GET">
                    <ns2:doc>Returns an audit history listing all organisational access changes made to a dataset</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A list of history elements</ns2:doc>
                        <ns2:representation element="List&lt;OrganisationAccessRequestAuditHistory&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
        </ns2:resource>
        <ns2:resource path="/taxonObservations">
            <ns2:method id="getObservationsByFilter" name="GET">
                <ns2:doc>Returns a list of Taxon Observations matching the given serach parameters</ns2:doc>
                <ns2:request>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="startYear" style="query" type="xs:int" default="-1">
                        <ns2:doc>The start year of the desired range</ns2:doc>
                    </ns2:param>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="endYear" style="query" type="xs:int" default="-1">
                        <ns2:doc>The end year of the desired range</ns2:doc>
                    </ns2:param>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="datasetKey" style="query" type="xs:string" default="">
                        <ns2:doc>Datasets to search in</ns2:doc>
                    </ns2:param>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="ptvk" style="query" type="xs:string" default="">
                        <ns2:doc>Taxon Version Keys to search for</ns2:doc>
                    </ns2:param>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="spatialRelationship" style="query" type="xs:string" default="overlap">
                        <ns2:doc>Any spatial relationship information required</ns2:doc>
                    </ns2:param>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="featureID" style="query" type="xs:string" default="">
                        <ns2:doc>Any required feature ID</ns2:doc>
                    </ns2:param>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="sensitive" style="query" type="xs:boolean" default="1">
                        <ns2:doc>If the results should include sensitive records or not</ns2:doc>
                    </ns2:param>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="designation" style="query" type="xs:string" default="">
                        <ns2:doc>Any required designations</ns2:doc>
                    </ns2:param>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonOutputGroup" style="query" type="xs:string" default="">
