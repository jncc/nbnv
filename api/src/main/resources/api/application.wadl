                    <ns2:doc>Filter ID for a TaxonObservationFilter</ns2:doc>
                </ns2:param>
                <ns2:method id="updateRequest" name="POST">
                    <ns2:doc>Update a specified Organisation Access Request</ns2:doc>
                    <ns2:request>
                        <ns2:representation mediaType="application/x-www-form-urlencoded">
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="action" style="query" type="xs:string">
<ns2:doc>The action to take on this request</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="reason" style="query" type="xs:string">
<ns2:doc>The reason this action has been taken</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="expires" style="query" type="xs:string" default="">
<ns2:doc>An optional expiry date for the access</ns2:doc>
                            </ns2:param>
                        </ns2:representation>
                    </ns2:request>
                    <ns2:response status="200">
                        <ns2:doc>A Response object detailing the success or failure of the action</ns2:doc>
                        <ns2:representation element="Response" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
                <ns2:method id="getRequest" name="GET">
                    <ns2:doc>Returns an Organisation Access Request with a specific filter ID</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>An Organisation Access Request with a specific filter ID</ns2:doc>
                        <ns2:representation element="OrganisationAccessRequest" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
                <ns2:method id="editRequest" name="PUT">
