                <ns2:method id="createRequest" name="PUT">
                    <ns2:doc>Create an Organisation Access Request for data</ns2:doc>
                    <ns2:request>
                        <ns2:representation mediaType="application/json"/>
                    </ns2:request>
                    <ns2:response status="200">
                        <ns2:doc>A Response object detailing the success or failure of the action</ns2:doc>
                        <ns2:representation element="Response" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="{id}/requests/granted">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:int">
                    <ns2:doc>An Organisation ID</ns2:doc>
                </ns2:param>
                <ns2:method id="getGrantedRequests" name="GET">
                    <ns2:doc>Returns a list of all Organisation Access Requests for a given 
