                    <ns2:doc>The dataset to be searched</ns2:doc>
                </ns2:param>
                <ns2:method id="searchSiteDataset" name="GET">
                    <ns2:doc>Search inside the site dataset records and a specific dataset for a given 
 search term</ns2:doc>
                    <ns2:request>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="rows" style="query" type="xs:int" default="10">
                            <ns2:doc>The page that the user wants to start displaying the results 
 at</ns2:doc>
                        </ns2:param>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="start" style="query" type="xs:int" default="0">
                            <ns2:doc>If we should sort the results or not</ns2:doc>
                        </ns2:param>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="bbox" style="query" type="xs:string" default="-180,-90,180,90">
                            <ns2:doc>A Bounding Box to search within</ns2:doc>
