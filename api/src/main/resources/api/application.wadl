            <ns2:resource path="/{id}/metadata">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:int">
                    <ns2:doc>The id of an organisation</ns2:doc>
                </ns2:param>
                <ns2:method id="getFullMetadata" name="GET">
                    <ns2:doc>Return a specified organisation from the core database, if the user is a
 administrator of the specified organisation or return a 403 Forbidden
 error</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A specified organisation from the core database</ns2:doc>
                        <ns2:representation element="Organisation" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
                <ns2:method id="updateOrganistionMetadata" name="POST">
                    <ns2:doc>Update the details of an organisation, if the user is an admin of the 
 specified organisation, otherwise return a 403 Forbidden error</ns2:doc>
                    <ns2:request>
                        <ns2:representation mediaType="application/x-www-form-urlencoded">
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="name" style="query" type="xs:string">
<ns2:doc>The name of the organisation</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="abbreviation" style="query" type="xs:string">
<ns2:doc>The abbreviation of the organisation</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="summary" style="query" type="xs:string">
<ns2:doc>A summary about the organisation</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="address" style="query" type="xs:string">
<ns2:doc>An address for the organisation</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="postcode" style="query" type="xs:string">
<ns2:doc>A postcode for the organisation</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="phone" style="query" type="xs:string">
<ns2:doc>A contact phone number for the organisation</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="website" style="query" type="xs:string">
<ns2:doc>The organisations website address</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="contactName" style="query" type="xs:string">
<ns2:doc>A contact for this organisation for the NBN</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="contactEmail" style="query" type="xs:string">
<ns2:doc>A contact email for this organisation for the NBN</ns2:doc>
                            </ns2:param>
                            <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="allowPublicRegistration" style="query" type="xs:string">
<ns2:doc>Whether this organisation allows public
 registration or not (is passed as the string 'on' due to jQuery 
 serialisation)</ns2:doc>
                            </ns2:param>
                        </ns2:representation>
                    </ns2:request>
                    <ns2:response>
                        <ns2:representation mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{id}/datasets">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:int">
                    <ns2:doc>An organisation ID</ns2:doc>
                </ns2:param>
                <ns2:method id="getDatasetsByID" name="GET">
