                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{id}/recordsPerDateType">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:string">
                    <ns2:doc>A dataset key</ns2:doc>
                </ns2:param>
                <ns2:method id="getDateTypeRecordCountsByDatasetKey" name="GET">
                    <ns2:doc>Return a list of records per date for a specific dataset</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A list of records per date for a specific dataset</ns2:doc>
                        <ns2:representation element="JSONObject" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{id}/attributes">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:string">
                    <ns2:doc>A dataset key</ns2:doc>
                </ns2:param>
                <ns2:method id="getAttributesByDatasetKey" name="GET">
                    <ns2:doc>Return a list of attributes in a specific dataset</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A list of attributes in a specific dataset</ns2:doc>
                        <ns2:representation element="JSONObject" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{id}">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:string">
                    <ns2:doc>A Taxon Dataset ID</ns2:doc>
                </ns2:param>
                <ns2:method id="getTaxonDatasetByID" name="GET">
                    <ns2:doc>Return a specific Taxon Dataset from the data warehouse</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A specific Taxon Dataset from the data warehouse</ns2:doc>
                        <ns2:representation element="TaxonDataset" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{id}/taxa">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:string">
                    <ns2:doc>A Taxon Dataset ID</ns2:doc>
                </ns2:param>
                <ns2:method id="getTaxaByDatasetKey" name="GET">
                    <ns2:doc>Return a list of Dataset Stats of the Taxon within a Taxon Dataset from 
