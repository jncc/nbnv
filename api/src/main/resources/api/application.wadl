                    <ns2:doc>Returns a Designation associated with a specified category</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A Designation associated with a specified category</ns2:doc>
                        <ns2:representation element="Designation" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{id}">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:int">
                    <ns2:doc>ID of a designation category</ns2:doc>
                </ns2:param>
                <ns2:method id="getDesignationCategoryByID" name="GET">
                    <ns2:doc>Returns a specified designation category from the data warehouse</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A specific designation category from the data warehouse</ns2:doc>
                        <ns2:representation element="DesignationCategory" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{id}/designations">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:int">
                    <ns2:doc>ID of a designation category</ns2:doc>
                </ns2:param>
                <ns2:method id="getDesignationByCategoryID" name="GET">
                    <ns2:doc>Returns a list of Designations associated with a specified category</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A list of Designations associated with a specified category</ns2:doc>
                        <ns2:representation element="List&lt;Designation&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
        </ns2:resource>
        <ns2:resource path="/taxa">
            <ns2:method id="getTaxa" name="GET">
                <ns2:doc>Search for a Taxon Record given a search term, this is a SOLR search
 function</ns2:doc>
                <ns2:request>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="rows" style="query" type="xs:int" default="20">
                        <ns2:doc>The number of rows to display per page</ns2:doc>
                    </ns2:param>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="start" style="query" type="xs:int" default="0">
                        <ns2:doc>The starting page</ns2:doc>
                    </ns2:param>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonOutputGroupKey" style="query" type="xs:string">
                        <ns2:doc>A List of Taxon Output Groups</ns2:doc>
                    </ns2:param>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="sort" style="query" type="xs:string">
                        <ns2:doc>Whether we should sort the results</ns2:doc>
                    </ns2:param>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="order" style="query" type="xs:string" default="asc">
                        <ns2:doc>The order in which we should sort the results</ns2:doc>
                    </ns2:param>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="prefered" style="query" type="xs:boolean" default="false">
                        <ns2:doc>If we should restrict the results to preferred taxon 
 records</ns2:doc>
                    </ns2:param>
                    <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="q" style="query" type="xs:string">
                        <ns2:doc>A given search term</ns2:doc>
                    </ns2:param>
                </ns2:request>
                <ns2:response status="200">
                    <ns2:doc>Search results for a given search term</ns2:doc>
                    <ns2:representation element="SolrResponse" mediaType="application/json;charset=utf-8"/>
                </ns2:response>
            </ns2:method>
            <ns2:resource path="/{taxonVersionKey}/datasets">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonVersionKey" style="template" type="xs:string">
                    <ns2:doc>A Taxon Version Key denoting a Taxon Record</ns2:doc>
                </ns2:param>
                <ns2:method id="getDatasetListForTaxonViewableByUser" name="GET">
                    <ns2:doc>Return a list of datasets that are viewable by the user and contain 
 records with a specified Taxon Version Key</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A List of Datasets containing records with a specified Taxon 
 Version Key</ns2:doc>
                        <ns2:representation element="List&lt;Dataset&gt;" mediaType="application/json;charset=utf-8"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{taxonVersionKey}/siteBoundaries">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonVersionKey" style="template" type="xs:string">
                    <ns2:doc>Taxon Version Keys to search for</ns2:doc>
                </ns2:param>
                <ns2:method id="getSiteBoundaries" name="GET">
                    <ns2:doc>Return a list of Site Boundaries containing Taxon records defined by the 
 given search parameters</ns2:doc>
                    <ns2:request>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="startYear" style="query" type="xs:int" default="-1">
                            <ns2:doc>The start year of the desired search range</ns2:doc>
                        </ns2:param>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="endYear" style="query" type="xs:int" default="-1">
                            <ns2:doc>The end year of the desired search range</ns2:doc>
                        </ns2:param>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="datasetKey" style="query" type="xs:string" default="">
                            <ns2:doc>Datasets to search in</ns2:doc>
                        </ns2:param>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="spatialRelationship" style="query" type="xs:string" default="overlap">
                            <ns2:doc>Any spatial relationship requirements</ns2:doc>
                        </ns2:param>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="featureID" style="query" type="xs:string" default="">
                            <ns2:doc>Any feature ID's required</ns2:doc>
                        </ns2:param>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="sensitive" style="query" type="xs:boolean" default="1">
                            <ns2:doc>If the records should be sensitive or not</ns2:doc>
                        </ns2:param>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="designation" style="query" type="xs:string" default="">
                            <ns2:doc>Any required designations</ns2:doc>
                        </ns2:param>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonOutputGroup" style="query" type="xs:string" default="">
                            <ns2:doc>Any Taxon Output Group required</ns2:doc>
                        </ns2:param>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="gridRef" style="query" type="xs:string" default="">
                            <ns2:doc>A Grid Reference to search within</ns2:doc>
                        </ns2:param>
                    </ns2:request>
                    <ns2:response status="200">
                        <ns2:doc>A List of Site Boundaries matching the given parameters</ns2:doc>
                        <ns2:representation element="List&lt;SiteBoundary&gt;" mediaType="application/json;charset=utf-8"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="{taxonVersionKey}">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonVersionKey" style="template" type="xs:string">
                    <ns2:doc>A Taxon Version Key denoting a Taxon Record</ns2:doc>
                </ns2:param>
                <ns2:method id="getTaxon" name="GET">
                    <ns2:doc>Return a specific Taxon record from the data warehouse</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A Taxon Record</ns2:doc>
                        <ns2:representation element="Taxon" mediaType="application/json;charset=utf-8"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="{taxonVersionKey}/synonyms">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonVersionKey" style="template" type="xs:string">
                    <ns2:doc>A Taxon Version Key denoting a Taxon Record</ns2:doc>
                </ns2:param>
                <ns2:method id="getTaxonSynonyms" name="GET">
                    <ns2:doc>Return a list of Taxon Records which are synonymous with the specified 
 Taxon Record</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A List of Synonyms for a given Taxon Version Key</ns2:doc>
                        <ns2:representation element="List&lt;Taxon&gt;" mediaType="application/json;charset=utf-8"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="{taxonVersionKey}/parent">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonVersionKey" style="template" type="xs:string">
                    <ns2:doc>A Taxon Version Key denoting a Taxon Record</ns2:doc>
                </ns2:param>
                <ns2:method id="getTaxonParent" name="GET">
                    <ns2:doc>Return the parent Taxon Record of a specific Taxon Record</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>The parent of the specified Taxon Record</ns2:doc>
                        <ns2:representation element="List&lt;Taxon&gt;" mediaType="application/json;charset=utf-8"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="{taxonVersionKey}/children">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonVersionKey" style="template" type="xs:string">
                    <ns2:doc>A Taxon Version Key denoting a Taxon Record</ns2:doc>
                </ns2:param>
                <ns2:method id="getTaxonChildren" name="GET">
                    <ns2:doc>Return a list of all Taxon that are children of a specific Taxon Record</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>The children of the specified Taxon Record</ns2:doc>
                        <ns2:representation element="List&lt;Taxon&gt;" mediaType="application/json;charset=utf-8"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="{taxonVersionKey}/taxonomy">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonVersionKey" style="template" type="xs:string">
                    <ns2:doc>A Taxon Version Key denoting a Taxon Record</ns2:doc>
                </ns2:param>
                <ns2:method id="getTaxonAncestry" name="GET">
                    <ns2:doc>Return a list of Taxon Records denoting the Ancestry of a specified 
 Taxon Record</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>The Ancestry of a given Taxon Record</ns2:doc>
                        <ns2:representation element="List&lt;Taxon&gt;" mediaType="application/json;charset=utf-8"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="{taxonVersionKey}/designations">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonVersionKey" style="template" type="xs:string">
                    <ns2:doc>A Taxon Version Key denoting a Taxon Record</ns2:doc>
                </ns2:param>
                <ns2:method id="getTaxonDesignations" name="GET">
                    <ns2:doc>Return a list of Taxon Designations that are applicable to a specified
 Taxon Record</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A List of Taxon Designations that apply to a specified record</ns2:doc>
                        <ns2:representation element="List&lt;TaxonDesignation&gt;" mediaType="application/json;charset=utf-8"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="{taxonVersionKey}/designations/archive">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonVersionKey" style="template" type="xs:string">
                    <ns2:doc>A Taxon Version Key denoting a Taxon Record</ns2:doc>
                </ns2:param>
                <ns2:method id="getArchiveTaxonDesignations" name="GET">
                    <ns2:doc>Return a list of Archive Taxon Designations that are applicable to a 
 specified Taxon Record</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A List of Archive Taxon Designations that apply to a specified 
 record</ns2:doc>
                        <ns2:representation element="List&lt;TaxonDesignation&gt;" mediaType="application/json;charset=utf-8"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{taxonVersionKey}/weblinks">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonVersionKey" style="template" type="xs:string">
                    <ns2:doc>A Taxon Version Key denoting a Taxon Record</ns2:doc>
                </ns2:param>
                <ns2:method id="getTaxonWebLinks" name="GET">
                    <ns2:doc>Return a list of Active Taxon Web Links for a specific Taxon Record</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A list of web links for a specific taxon record</ns2:doc>
                        <ns2:representation element="List&lt;TaxonWebLink&gt;" mediaType="application/json;charset=utf-8"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{taxonVersionKey}/siteBoundaries/download">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonVersionKey" style="template" type="xs:string"/>
                <ns2:method id="getSiteBoundariesDownload" name="GET">
                    <ns2:doc>Return a zipped copy of a list of Site Boundaries containing Taxon 
 records defined by the given search parameters</ns2:doc>
                    <ns2:request>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="startYear" style="query" type="xs:int" default="-1"/>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="endYear" style="query" type="xs:int" default="-1"/>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="datasetKey" style="query" type="xs:string" default=""/>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="spatialRelationship" style="query" type="xs:string" default="overlap"/>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="featureID" style="query" type="xs:string" default=""/>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="sensitive" style="query" type="xs:boolean" default="1"/>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="designation" style="query" type="xs:string" default=""/>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonOutputGroup" style="query" type="xs:string" default=""/>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="gridRef" style="query" type="xs:string" default=""/>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="polygon" style="query" type="xs:string" default=""/>
                    </ns2:request>
                    <ns2:response status="200">
                        <ns2:doc>A zipped download containing list of Site Boundaries matching the 
 given parameters</ns2:doc>
                        <ns2:representation element="StreamingOutput" mediaType="application/x-zip-compressed"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
        </ns2:resource>
        <ns2:resource path="/habitatDatasets">
            <ns2:method id="get" name="GET">
                <ns2:doc>Returns a list of all habitat datasets from the data warehouse</ns2:doc>
                <ns2:response status="200">
                    <ns2:doc>A list of all habitat datasets from the data warehouse</ns2:doc>
                    <ns2:representation element="List&lt;HabitatDataset&gt;" mediaType="application/json"/>
                </ns2:response>
            </ns2:method>
            <ns2:resource path="/{id}">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:string">
                    <ns2:doc>A Habitat Dataset Key</ns2:doc>
                </ns2:param>
                <ns2:method id="getByDatasetKey" name="GET">
                    <ns2:doc>Returns a specific habitat dataset from the data warehouse</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A specific habitat dataset from the data warehouse</ns2:doc>
                        <ns2:representation element="HabitatDataset" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
        </ns2:resource>
        <ns2:resource path="/designations">
            <ns2:method id="getDesignationList" name="GET">
                <ns2:doc>Return a list of all Designations from the data warehouse</ns2:doc>
                <ns2:response status="200">
                    <ns2:doc>A list of all Designations from the data warehouse</ns2:doc>
                    <ns2:representation element="List&lt;Designation&gt;" mediaType="application/json"/>
                </ns2:response>
            </ns2:method>
            <ns2:resource path="{id}/taxonNavigationGroups/{taxonNavigationGroupId}">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:string">
                    <ns2:doc>A Designation ID</ns2:doc>
                </ns2:param>
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonNavigationGroupId" style="template" type="xs:string">
                    <ns2:doc>A Taxon Navigation Group ID</ns2:doc>
                </ns2:param>
                <ns2:method id="getTaxonNavigationGroupByDesignation" name="GET">
                    <ns2:doc>Returns a Taxon Navigation Group specified by a designation and taxon 
 navigation group id</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A Taxon Navigation Group specified by a designation and taxon 
 navigation group ID</ns2:doc>
                        <ns2:representation element="TaxonNavigationGroup" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{id}/designationCategories">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:string">
                    <ns2:doc>A Designation ID</ns2:doc>
                </ns2:param>
                <ns2:method id="getDesignationCategory" name="GET">
                    <ns2:doc>Returns all associated designation categories associated with a specified 
 designation</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>All associated designation categories associated with a specified 
 designation</ns2:doc>
                        <ns2:representation element="DesignationCategory" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{id}">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:string">
                    <ns2:doc>A Designation ID</ns2:doc>
                </ns2:param>
                <ns2:method id="getDesignation" name="GET">
                    <ns2:doc>Returns a specific designation from the data warehouse</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A specific designation from the data warehouse</ns2:doc>
                        <ns2:representation element="Designation" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{id}/datasets">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:string">
                    <ns2:doc>A Designation ID</ns2:doc>
                </ns2:param>
                <ns2:method id="getDesignationDatasets" name="GET">
                    <ns2:doc>Returns a list of datasets in a specified designation which are viewable
 by the current user</ns2:doc>
                    <ns2:response status="200">
                        <ns2:doc>A list of datasets in a specified designation which are viewable
 by the current user</ns2:doc>
                        <ns2:representation element="List&lt;Dataset&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{id}/species">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:string">
                    <ns2:doc>A Designation ID</ns2:doc>
                </ns2:param>
                <ns2:method id="getSpeciesByDesignationAndTaxonNavigationGroup" name="GET">
                    <ns2:doc>Returns a list of Taxons specified by designation and a taxon navigation
 group (or just by a designation id if no taxon navigation group id is 
 supplied)</ns2:doc>
                    <ns2:request>
                        <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxonNavigationGroupId" style="query" type="xs:string">
                            <ns2:doc>A Taxon Navigation Group ID (Optional) 
 supplied via querystring</ns2:doc>
                        </ns2:param>
                    </ns2:request>
                    <ns2:response status="200">
                        <ns2:doc>A list of Taxons specified by designation and a taxon navigation
 group (or just by a designation id if no taxon navigation group id is 
 supplied)</ns2:doc>
                        <ns2:representation element="List&lt;Taxon&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
        </ns2:resource>
    </ns2:resources>
</ns2:application>
