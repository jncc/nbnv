                        <ns2:doc>A List of Organisation Access Requests which have been granted 
 and the current user has admin rights over</ns2:doc>
                        <ns2:representation element="List&lt;OrganisationAccessRequest&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
                <ns2:method id="createGrant" name="PUT">
                    <ns2:request>
                        <ns2:representation mediaType="application/json"/>
                    </ns2:request>
                    <ns2:response>
                        <ns2:representation mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/requests/admin/denied">
                <ns2:method id="getRequestsDeniedForAdmin" name="GET">
                    <ns2:doc>Returns a List of Organisation Access Requests which have been denied 
