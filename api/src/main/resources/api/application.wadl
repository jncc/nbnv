                        <ns2:representation element="List&lt;Dataset&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
            </ns2:resource>
            <ns2:resource path="/{id}">
                <ns2:param xmlns:xs="http://www.w3.org/2001/XMLSchema" name="id" style="template" type="xs:int">
                    <ns2:doc>An organisation ID</ns2:doc>
                </ns2:param>
                <ns2:method id="getByID" name="GET">
                    <ns2:doc>Return a specified organisation</ns2:doc>
