                    <ns2:response status="200">
                        <ns2:doc>A List of Organisation Access Requests for which the current user
 has admin rights</ns2:doc>
                        <ns2:representation element="List&lt;OrganisationAccessRequest&gt;" mediaType="application/json"/>
                    </ns2:response>
                </ns2:method>
